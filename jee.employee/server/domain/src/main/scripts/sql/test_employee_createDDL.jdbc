CREATE TABLE JOBTITLE (JOB_ID BIGINT NOT NULL, TITLE VARCHAR(255), PRIMARY KEY (JOB_ID))
CREATE TABLE DEGREE (DEGREE_ID BIGINT NOT NULL, NAME VARCHAR(255), EMP_ID BIGINT, PRIMARY KEY (DEGREE_ID))
CREATE TABLE EMP_JOB (EMP_ID BIGINT NOT NULL, TITLE_ID BIGINT NOT NULL, PRIMARY KEY (EMP_ID, TITLE_ID))
CREATE TABLE EMAIL (EMAIL_TYPE VARCHAR(255), EMAIL_ADDRESS VARCHAR(255), EMP_ID BIGINT)
CREATE TABLE RESPONS (EMP_ID BIGINT, RESPONSIBILITY VARCHAR(255), PRIORITY INTEGER)
CREATE TABLE PROJ_EMP (EMP_ID BIGINT NOT NULL, PROJ_ID INTEGER NOT NULL, PRIMARY KEY (EMP_ID, PROJ_ID))
